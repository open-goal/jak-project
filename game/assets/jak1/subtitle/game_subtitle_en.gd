  (1169 :offscreen "SAGE" "FOLLOW THE LAMPS, THEY'LL TAKE YOU RIGHT THERE.")
  (1310)
  (1313 :offscreen "SAGE" "NOW, ALL OF YOU...")
  (1415)
  (1439 :offscreen "SAGE" "GET OUT OF HERE!")
  )

