  (1496 :offscreen "SAMOS" "ABER ER LEBT WEIT ENTFERNT IM NORDEN. WEIT, WEIT IM NORDEN.")
  (1649 :offscreen "SAMOS" "SEIT EWIGKEITEN HAT NIEMAND MEHR MIT IHM GESPROCHEN.")
