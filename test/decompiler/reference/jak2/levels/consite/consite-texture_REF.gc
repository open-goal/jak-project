      (new 'static 'texture-anim
        :num-layers #x2
        :func #f
        :init-func #f
        :tex #f
        :tex-name "kor-eyeeffect-formorph"
        :color (new 'static 'rgba :a #x80)
        :frame-mod 2.0
        :test (new 'static 'gs-test :ate #x1 :afail #x1 :zte #x1 :ztst (gs-ztest always))
        :alpha (new 'static 'gs-alpha :b #x1 :d #x1)
        )
      (new 'static 'texture-anim
