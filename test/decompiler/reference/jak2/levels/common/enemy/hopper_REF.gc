    (((knocked-type knocked-type-6))
     (let* ((a2-0 (ash 1 (-> *hopper-global-info* prev-blue-hit)))
            (v1-3 (rnd-bit this 3 a2-0))
            (a1-5 (-> this draw art-group data (-> *hopper-global-info* blue-hit-anim v1-3 hit-anim-index)))
            )
