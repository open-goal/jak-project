  (let* ((s5-1 (vector-! (new 'stack-no-clear 'vector) (target-pos 0) (-> this root trans)))
         (f30-0 (vector-dot (vector-x-quaternion! (new 'stack-no-clear 'vector) (-> this root quat)) s5-1))
         (f0-2 (vector-dot (vector-z-quaternion! (new 'stack-no-clear 'vector) (-> this root quat)) s5-1))
         )
    (and (< (fabs f30-0) 16384.0) (if (-> this open-side)
                                      (and (< f0-2 0.0) (< -32768.0 f0-2))
                                      (and (< 0.0 f0-2) (< f0-2 32768.0))
                                      )
         )
    )
  )

