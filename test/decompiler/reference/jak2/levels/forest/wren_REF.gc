    (set-vector! (-> obj root scale) f0-0 f0-0 f0-0 1.0)
    )
  (set! (-> obj fly-index) (the-as uint 0))
  (set! (-> obj path-u) 0.0)
  (set! (-> obj flags) (wren-flags))
  (set! (-> obj bob-level) 0.0)
  (set! (-> obj bank-angle) 0.0)
  (set! (-> obj path) (new 'process 'path-control obj 'idle 0.0 arg0 #f))
