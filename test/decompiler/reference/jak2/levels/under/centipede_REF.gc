;; definition of type centipede-cam
(deftype centipede-cam (structure)
  ((init?             symbol)
   (trans             vector  :inline)
   (dir               vector  :inline)
   (track-focus-tilt  float)
   )
  )

;; definition for method 3 of type centipede-cam
(defmethod inspect ((this centipede-cam))
  (when (not this)
    (set! this this)
    (goto cfg-4)
    )
  (format #t "[~8x] ~A~%" this 'centipede-cam)
  (format #t "~1Tinit?: ~A~%" (-> this init?))
  (format #t "~1Ttrans: #<vector @ #x~X>~%" (-> this trans))
  (format #t "~1Tdir: #<vector @ #x~X>~%" (-> this dir))
  (format #t "~1Ttrack-focus-tilt: ~f~%" (-> this track-focus-tilt))
  (label cfg-4)
  this
