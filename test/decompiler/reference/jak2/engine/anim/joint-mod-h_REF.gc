    (set! (-> v0-0 wheel-radius) arg2)
    (set! (-> v0-0 wheel-axis) arg3)
    (set! (-> v0-0 angle) 0.0)
    (set-vector! (-> v0-0 last-position) 0.0 0.0 0.0 1.0)
    (let ((a0-3 (-> arg0 node-list data arg1)))
      (set! (-> a0-3 param0) joint-mod-wheel-callback)
      (set! (-> a0-3 param1) v0-0)
      )
    v0-0
    )
