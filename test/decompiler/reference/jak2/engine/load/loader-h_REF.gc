  (format #t "~1Tdma-reserve-buffer-count: ~D~%" (-> obj dma-reserve-buffer-count))
  (format #t "~1Tactive-stream: ~A~%" (-> obj active-stream))
  (format #t "~1Tqueue-stream: ~A~%" (-> obj queue-stream))
