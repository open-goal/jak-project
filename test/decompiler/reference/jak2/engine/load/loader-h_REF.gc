  (format #t "~1Tpending-load-file: ~A~%" (-> obj pending-load-file))
  (format #t "~1Tpending-load-file-part: ~D~%" (-> obj pending-load-file-part))
  (format #t "~1Tpending-load-file-owner: ~D~%" (-> obj pending-load-file-owner))
