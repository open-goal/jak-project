  (format #t "~1Tload-file-owner: ~D~%" (-> obj load-file-owner))
  (format #t "~1Tload-file-priority: ~f~%" (-> obj load-file-priority))
  (format #t "~1Tbuf: #x~X~%" (-> obj buf))
  (format #t "~1Tlen: ~D~%" (-> obj len))
  (format #t "~1Tart-group: ~A~%" (-> obj art-group))
