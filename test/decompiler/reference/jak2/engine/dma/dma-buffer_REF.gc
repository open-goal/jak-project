    )
  (format #t "[~8x] ~A~%" obj 'dma-gif-packet)
  (format #t "~1Tdma-vif: #<dma-packet @ #x~X>~%" (-> obj dma-vif))
  (format #t "~1Tgif[2] @ #x~X~%" (&-> obj gif0))
  (format #t "~1Tquad[2] @ #x~X~%" (-> obj dma-vif))
