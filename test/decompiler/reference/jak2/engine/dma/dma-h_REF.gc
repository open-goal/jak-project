  ((tag    dma-tag)
   (last   (pointer dma-tag))
   (dummy  uint32)
   (next   uint32  :offset   4)
   (clear  uint64  :overlay-at last)
   (vif0   uint32  :overlay-at last)
   (vif1   uint32  :overlay-at dummy)
   )
  )

