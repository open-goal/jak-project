  (format #t "[~8x] ~A~%" this 'wind-dma)
  (format #t "~1Tbuffer0[128] @ #x~X~%" (-> this buffer0))
  (format #t "~1Tbuffer1[128] @ #x~X~%" (-> this buffer1))
  (format #t "~1Tbuffer2[128] @ #x~X~%" (-> this buffer2))
