  (set! (-> this target-interp fog) fog-target)
  (set! (-> this speed-interp cloud) cloud-speed)
  (set! (-> this speed-interp fog) fog-speed)
  (if (= cloud-speed 0.0)
      (set! (-> this current-interp cloud) cloud-target)
      )
  (if (= fog-speed 0.0)
      (set! (-> this current-interp fog) fog-target)
      )
  0
  (none)
  )

