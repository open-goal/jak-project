      (nop!)
      0
      )
    (link-texture-by-id (-> shader texture-id) shader)
    (let ((s5-0 (lookup-texture-by-id (-> shader texture-id))))
      (cond
        (s5-0
          (if (and *debug-segment* (-> *screen-shot-work* highres-enable))
              (hack-texture s5-0)
