          (adgif-shader<-texture-with-update! shader s5-0)
          )
        (else
          (format
            0
            "login<1> could not find texture ~X in obj ~A shader ~X~%"
            (-> shader texture-id)
            (-> *kernel-context* login-object)
            shader
            )
