;; definition for method 3 of type target
(defmethod inspect target ((obj target))
  (when (not obj)
    (set! obj obj)
    (goto cfg-4)
    )
  (let ((t9-0 (method-of-type process-focusable inspect)))
    (t9-0 obj)
    )
  (format #t "~2Tcontrol: ~A~%" (-> obj control))
  (format #t "~2Tskel2: ~A~%" (-> obj skel2))
