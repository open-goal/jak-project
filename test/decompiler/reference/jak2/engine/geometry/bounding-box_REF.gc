       (< (-> this min y) (-> arg0 y))
       (< (-> this min z) (-> arg0 z))
       (< (-> arg0 x) (-> this max x))
       (< (-> arg0 y) (-> this max y))
       (< (-> arg0 z) (-> this max z))
       )
  )

;; definition for method 20 of type bounding-box
(defmethod inside-xz? ((this bounding-box) (arg0 vector))
