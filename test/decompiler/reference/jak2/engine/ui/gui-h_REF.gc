  (format #t "~1Tquad[2] @ #x~X~%" (&-> obj next0))
  (format #t "~1Tpriority: ~f~%" (-> obj priority))
  (let ((t9-11 format)
        (a0-12 #t)
        (a1-11 "~1Taction: #x~X : ~S~%")
