    (set! obj obj)
    (goto cfg-116)
    )
  (format #t "[~8x] ~A~%" obj 'gui-connection)
  (format #t "~1Tnext0: ~`connectable`P~%" (-> obj next0))
  (format #t "~1Tprev0: ~`connectable`P~%" (-> obj prev0))
