    (ja-channel-set! 0)
    (ja-post)
    (send-event *camera* 'change-state cam-fixed 0)
    )
  :exit (behavior ()
    (when (not (or (= (-> self next-state name) 'target-title-play) (= (-> self next-state name) 'target-title-wait)))
      (if (-> self manipy)
          (deactivate (-> self manipy 0))
          )
      )
    )
  :trans (behavior ()
    (hide-hud-quick)
    (spool-push *art-control* "ndi-intro" 0 self -1.0)
