    (('attack)
     (nav-enemy-set-hit-from-direction arg0)
     (go-virtual nav-enemy-die)
     )
    (('touch)
     (nav-enemy-send-attack arg0 (the-as touching-shapes-entry (-> arg3 param 0)) 'generic)
     )
    )
  )

