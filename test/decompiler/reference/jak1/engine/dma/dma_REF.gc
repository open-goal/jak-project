(defun dma-send ((arg0 dma-bank) (arg1 uint) (arg2 uint))
  (dma-sync (the-as pointer arg0) 0 0)
  (flush-cache 0)
  (.sync.l)
  (set! (-> arg0 madr) (logior (logand #xfffffff arg1) (if (= (logand #x70000000 arg1) #x70000000)
                                                           (shl #x8000 16)
                                                           0
                                                           )
                               )
        )
  (set! (-> arg0 qwc) arg2)
  (.sync.l)
  (set! (-> arg0 chcr) (new 'static 'dma-chcr :str #x1))
  (.sync.l)
  0
  (none)
  )

