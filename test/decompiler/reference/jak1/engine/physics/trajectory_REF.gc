(defmethod setup-from-to-height! ((this trajectory) (arg0 vector) (arg1 vector) (arg2 float) (arg3 float))
  (let* ((f1-2 (+ arg2 (fmax (-> arg0 y) (-> arg1 y))))
         (f1-5 (* 2.0 (- (-> arg0 y) f1-2) arg3))
         (f0-3 4096.0)
         )
    (if (< 0.0 f1-5)
        (set! f0-3 (sqrtf f1-5))
        )
    (setup-from-to-y-vel! this arg0 arg1 f0-3 arg3)
    )
  0
  (none)
  )

