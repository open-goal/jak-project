        (set! (-> dma vif1) (new 'static 'vif-tag :cmd (vif-cmd mpg) :num (* qwc-this-time 2) :imm dst))
        (&+! dma 16)
        (set! func-data (+ (the-as uint func-data) (* qwc-this-time 16)))
        (set! qwc (- qwc qwc-this-time))
        (+! dst (* qwc-this-time 2))
