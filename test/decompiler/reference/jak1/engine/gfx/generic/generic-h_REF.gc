   (x    uint32    :offset          0)
   (y    uint32    :offset          4)
   (z    uint32    :offset          8)
   (w    uint32    :offset         12)
   )
  :method-count-assert 9
  :size-assert         #x10
  :flag-assert         #x900000010
  )

;; definition for method 3 of type ad-cmd
