   (water-drip-mult     float                            :offset-assert 2396)
   (water-drip-speed    float                            :offset-assert 2400)
   )
  :heap-base #x900
  :method-count-assert 14
  :size-assert         #x964
  :flag-assert         #xe09000964
  (:states
    cam-master-active
    list-keeper-active
    )
  )

