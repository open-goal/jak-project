;;-*-Lisp-*-
(in-package goal)

;; definition of type sound-id
(deftype sound-id (uint32)
  ()
  :method-count-assert 10
  :size-assert         #x4
  :flag-assert         #xa00000004
  (:methods
    (unused-9 () none 9)
    )
  )

;; definition of type sound-bank-id
(deftype sound-bank-id (uint32)
