           )
         (cond
           (sv-48
             (set! (-> this time-base) (the-as time-frame (the int (* 300.0 (-> sv-48 0)))))
             (set! (-> this time-random) (the-as time-frame (the int (* 300.0 (-> sv-48 1)))))
             )
           (else
             (set! (-> this time-base) -1)
             )
           )
