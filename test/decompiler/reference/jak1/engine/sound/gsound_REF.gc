;; definition for method 3 of type sound-iop-info
(defmethod inspect ((this sound-iop-info))
  (format #t "[~8x] ~A~%" this (-> this type))
  (format #t "~Tframe: ~D~%" (-> this frame))
  (format #t "~Tstrpos: ~D~%" (-> this strpos))
  (format #t "~Tstr-id: ~D~%" (-> this str-id))
  (format #t "~Tstr-id-sign: ~D~%" (-> this str-id-sign))
  (format #t "~Tfreemem: ~D~%" (-> this freemem))
  (format #t "~Tchinfo[48] @ #x~X~%" (-> this chinfo))
  (format #t "~Tfreemem2: ~D~%" (-> this freemem2))
