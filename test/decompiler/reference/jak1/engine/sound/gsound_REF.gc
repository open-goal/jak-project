  (format #t "~Tlastspeed: ~D~%" (-> this lastspeed))
  (format #t "~Tdupseg: ~D~%" (-> this dupseg))
  (format #t "~Ttimes[41] @ #x~X~%" (-> this times))
  (format #t "~Ttimes-seq: ~D~%" (-> this times-seq))
