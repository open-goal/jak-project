  (format #t "~Tdrip-time: ~D~%" (-> obj drip-time))
  (format #t "~Tdrip-speed: ~f~%" (-> obj drip-speed))
  (format #t "~Tdrip-height: (meters ~m)~%" (-> obj drip-height))
  (format #t "~Tdrip-mult: ~f~%" (-> obj drip-mult))
