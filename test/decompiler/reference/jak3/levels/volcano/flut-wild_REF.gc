;;-*-Lisp-*-
(in-package goal)

;; definition of type flut-wild
(deftype flut-wild (nav-enemy)
  ((minimap             connection-minimap)
   (focus-ignore-timer  time-frame)
   (color-index         int32)
   (first-notice?       symbol)
   )
  (:state-methods
    flee-path
    disappear
    )
  )

;; definition for method 3 of type flut-wild
(defmethod inspect ((this flut-wild))
  (when (not this)
    (set! this this)
    (goto cfg-4)
    )
  (let ((t9-0 (method-of-type nav-enemy inspect)))
    (t9-0 this)
    )
  (format #t "~2Tminimap: #<connection-minimap @ #x~X>~%" (-> this minimap))
  (format #t "~2Tfocus-ignore-timer: ~D~%" (-> this focus-ignore-timer))
  (format #t "~2Tcolor-index: ~D~%" (-> this color-index))
  (format #t "~2Tfirst-notice?: ~A~%" (-> this first-notice?))
  (label cfg-4)
  this
  )

