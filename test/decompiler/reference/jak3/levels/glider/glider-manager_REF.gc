(defstate active (task-manager-desert-glide)
  :virtual #t
  :code (behavior ()
    (local-vars (v1-1 object))
    (suspend)
    (until v1-1
      (suspend)
      (set! v1-1 (and *target* (focus-test? *target* pilot)))
