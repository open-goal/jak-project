    (cond
      ((-> this player-ring?)
       (cond
         ((-> this is-final?)
          (if (nonzero? (-> this part-final))
              (spawn-from-mat (-> this part-final) (-> this mat))
              )
          )
         (else
           (if (nonzero? (-> this player-part))
               (spawn-from-mat (-> this player-part) (-> this mat))
               )
           )
         )
       )
      ((nonzero? (-> this part))
       (spawn-from-mat (-> this part) (-> this mat))
       )
      )
    )
  0
  (none)
  )

