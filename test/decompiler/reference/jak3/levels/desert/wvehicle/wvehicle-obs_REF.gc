;; definition of type turbo-pickup
(deftype turbo-pickup (process-drawable)
  ((root        collide-shape :override)
   (available   symbol)
   (persistent  symbol)
   (birth-time  time-frame)
   (collector   handle)
   )
  (:state-methods
    idle
    die
    )
  (:methods
    (find-ground (_type_) symbol)
    )
  )

;; definition for method 3 of type turbo-pickup
(defmethod inspect ((this turbo-pickup))
  (when (not this)
    (set! this this)
    (goto cfg-4)
    )
  (let ((t9-0 (method-of-type process-drawable inspect)))
    (t9-0 this)
    )
  (format #t "~2Tavailable: ~A~%" (-> this available))
  (format #t "~2Tpersistent: ~A~%" (-> this persistent))
  (format #t "~2Tbirth-time: ~D~%" (-> this birth-time))
  (format #t "~2Tcollector: ~D~%" (-> this collector))
  (label cfg-4)
  this
  )

