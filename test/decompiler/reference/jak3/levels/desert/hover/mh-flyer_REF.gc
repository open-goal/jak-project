  (format #t "~2Tfocus-xz-dir: #<vector @ #x~X>~%" (-> this focus-xz-dir))
  (format #t "~2Tminimap: #<connection-minimap @ #x~X>~%" (-> this minimap))
  (format #t "~2Tdes-path: #<desbeast-path @ #x~X>~%" (-> this des-path))
  (format #t "~2Tmanager: ~D~%" (-> this manager))
  (format #t "~2Tpath-pos: ~D~%" (-> this path-pos))
