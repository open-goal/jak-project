;; definition for method 3 of type mh-flyer
(defmethod inspect ((this mh-flyer))
  (when (not this)
    (set! this this)
    (goto cfg-4)
    )
  (let ((t9-0 (method-of-type enemy inspect)))
    (t9-0 this)
    )
  (format #t "~2Trotation-matrix: #<matrix @ #x~X>~%" (-> this rotation-matrix))
  (format #t "~2Tmove-curve: #<cubic-curve @ #x~X>~%" (-> this move-curve))
  (format #t "~2Tinit-pos: #<vector @ #x~X>~%" (-> this init-pos))
