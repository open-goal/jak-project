;;-*-Lisp-*-
(in-package goal)

;; definition for symbol *desert-lizard-almost-there-timer*, type time-frame
(define *desert-lizard-almost-there-timer* (the-as time-frame 0))

;; definition of type desert-lizard
(deftype desert-lizard (nav-enemy)
  ((graph         lizard-graph)
   (minimap       connection-minimap)
   (catch-timer   uint64)
   (closest-dist  float)
   (color-index   int32)
   (talker-id     uint32)
   )
  (:state-methods
    catching-daxter
    disappear
    )
  (:methods
    (can-be-mounted? (_type_ process-focusable float float float) symbol)
    )
  )

;; definition for method 3 of type desert-lizard
(defmethod inspect ((this desert-lizard))
  (when (not this)
    (set! this this)
    (goto cfg-4)
    )
  (let ((t9-0 (method-of-type nav-enemy inspect)))
    (t9-0 this)
    )
  (format #t "~2Tgraph: #<lizard-graph @ #x~X>~%" (-> this graph))
  (format #t "~2Tminimap: #<connection-minimap @ #x~X>~%" (-> this minimap))
  (format #t "~2Tcatch-timer: ~D~%" (-> this catch-timer))
  (format #t "~2Tclosest-dist: ~f~%" (-> this closest-dist))
  (format #t "~2Tcolor-index: ~D~%" (-> this color-index))
  (format #t "~2Ttalker-id: ~D~%" (-> this talker-id))
  (label cfg-4)
  this
  )

