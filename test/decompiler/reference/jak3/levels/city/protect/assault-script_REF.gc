  (format #t "~1Tstate-time: ~D~%" (-> this state-time))
  (format #t "~1Tcurrent-command: #<assault-spawn-cmd @ #x~X>~%" (-> this current-command))
  (format #t "~1Tscript: ~A~%" (-> this script))
  (label cfg-4)
