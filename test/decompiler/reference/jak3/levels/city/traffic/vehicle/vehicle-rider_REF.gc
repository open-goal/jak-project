    (let* ((s3-0 (get-process *default-dead-pool* arg1 #x4000 1))
           (v1-1 (when s3-0
                   (let ((t9-1 (method-of-type process activate)))
                     (t9-1 s3-0 arg0 "rider" (the-as pointer #x70004000))
                     )
                   (run-now-in-process s3-0 vehicle-rider-init-by-other arg2)
                   (-> s3-0 ppointer)
                   )
                 )
           )
      (if v1-1
          (set! gp-0 (-> v1-1 0))
          )
      )
    gp-0
    )
