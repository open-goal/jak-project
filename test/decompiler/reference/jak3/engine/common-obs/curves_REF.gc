;; definition for function rgbaf-lerp!
(defun rgbaf-lerp! ((arg0 rgbaf) (arg1 rgbaf) (arg2 rgbaf) (arg3 float))
  "Lerp all four components of rgba."
  (vector-lerp! arg0 arg1 arg2 arg3)
  (set! (-> arg0 w) (lerp (-> arg1 w) (-> arg2 w) arg3))
  arg0
  )

