            (set! (-> *camera-combiner* fov) (* 182.04445 arg2))
            )
        (if (and *camera* (-> *camera* slave))
            (set! (-> *camera* slave 0 fov) (* 182.04445 arg2))
            )
        )
      )
    )
  (cond
    ((= (the-as int arg0) 'fov)
     (cond
       ((get-setting *setting-control* 'fov)
        (empty)
        arg2
        )
       (*math-camera*
         (* 0.005493164 (-> *math-camera* fov))
         )
       (else
         arg3
         )
       )
     )
    (else
      arg3
      )
    )
  )

