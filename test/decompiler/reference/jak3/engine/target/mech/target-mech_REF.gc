            (if (or (and (< 0.0 f28-0)
                         (< 0.3 (-> self control pad-magnitude))
                         (< (vector-dot (-> self control pad-xz-dir) (-> self control last-pad-xz-dir)) 0.2)
                         (< f28-0 0.7)
                         )
                    (-> self mech stick-off)
                    )
