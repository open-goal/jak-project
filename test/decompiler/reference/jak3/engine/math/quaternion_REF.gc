         (vf2 :class vf)
         )
    (.lvf vf1 (&-> arg1 quad))
    (.mov vf2 arg2)
    (.mul.x.vf vf1 vf1 vf2)
    (.svf (&-> arg0 quad) vf1)
    arg0
    )
  )

