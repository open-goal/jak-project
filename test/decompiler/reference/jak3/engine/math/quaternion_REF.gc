         )
    (.lvf vf1 (&-> arg1 quad))
    (.lvf vf2 (&-> arg2 quad))
    (.sub.vf vf1 vf1 vf2)
    (.svf (&-> arg0 quad) vf1)
    arg0
    )
  )

