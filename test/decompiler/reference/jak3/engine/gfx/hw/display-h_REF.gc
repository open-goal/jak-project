                                           tie
                                           etie
                                           etie-scissor
                                           tie-vanish
                                           generic
                                           merc
                                           emerc
                                           shrubbery
                                           shrub-near
                                           billboard
                                           shrub-vanish
                                           tfrag-trans
                                           tie-scissor-trans
                                           tie-trans
                                           etie-trans
                                           etie-scissor-trans
                                           tfrag-water
                                           tie-scissor-water
                                           tie-water
                                           etie-water
                                           etie-scissor-water
                                           sprite
                                           rn32
                                           rn33
                                           rn34
                                           rn36
                                           )
          )
    (set! (-> gp-0 vu1-enable-user) (vu1-renderer-mask
                                      sky
                                      rn4
                                      rn5
                                      hfrag
                                      hfrag-scissor
                                      tfrag
                                      tie-scissor
                                      tie
                                      etie
                                      etie-scissor
                                      tie-vanish
                                      generic
                                      merc
                                      emerc
                                      shrubbery
                                      shrub-near
                                      billboard
                                      shrub-vanish
                                      tfrag-trans
                                      tie-scissor-trans
                                      tie-trans
                                      etie-trans
                                      etie-scissor-trans
                                      tfrag-water
                                      tie-scissor-water
                                      tie-water
                                      etie-water
                                      etie-scissor-water
                                      sprite
                                      rn32
                                      rn33
                                      rn34
                                      rn36
                                      )
          )
    gp-0
    )
  )

