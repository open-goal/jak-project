  (format #t "~1Tr-sun: ~f~%" (-> this r-sun))
  (format #t "~1Tr-halo: ~f~%" (-> this r-halo))
  (format #t "~1Tr-aurora: ~f~%" (-> this r-aurora))
  (format #t "~1Tc-sun-start: ~D~%" (-> this c-sun-start))
  (format #t "~1Tc-sun-end: ~D~%" (-> this c-sun-end))
