  (format #t "~Tprocess: ~A~%" (-> obj process))
  (format #t "~Tcurve: #<curve @ #x~X>~%" (&-> obj cverts))
  (format #t "~Tnum-cverts: ~D~%" (-> obj curve num-cverts))
  (format #t "~Tcverts: #x~X~%" (-> obj cverts))
