    )
   (((task-status need-reward-speech))
    (when arg0
     (set! (-> obj cell-for-task) (current-task (-> obj tasks)))
     (close-current! (-> obj tasks))
     (let ((s5-1 (get-process *default-dead-pool* manipy #x4000)))
