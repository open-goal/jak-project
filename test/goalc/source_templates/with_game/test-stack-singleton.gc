(defun stack-addr-test ()
  (let ((vec1 (new 'stack-no-clear 'vector))
        (vec2 (new 'stack-no-clear 'vector))
        (vec3 (new 'stack-singleton-no-clear 'vector))
        (vec4 (new 'stack 'vector))
        (vec5 (new 'stack-singleton-no-clear 'vector))
        )
    (format #t "~A ~A ~A ~A ~A~%"
            (= vec1 vec2)
            (= vec2 vec3)
            (= vec3 vec4)
            (= vec4 vec5)
            (= vec3 vec5)
            )
    )
  )

(stack-addr-test)