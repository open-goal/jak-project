(defun test-vector-constant ()
  (let ((vector-1 (new 'stack 'vector)))

    (set-vector! vector-1 1.0 2.0 3.0 4.0)

    (format #t "(~f, ~f, ~f, ~f)~%" (-> vector-1 x) (-> vector-1 y) (-> vector-1 z) (-> vector-1 w))

    (rlet ((vf1 :class vf :reset-here #t))
      (.lvf0 vf1)
      (.svf vector-1 vf1))

    (format #t "(~f, ~f, ~f, ~f)~%" (-> vector-1 x) (-> vector-1 y) (-> vector-1 z) (-> vector-1 w))))

(test-vector-constant)
