(defmacro is-nan-hack (flt)
  `(and (< 0.0 ,flt) (< ,flt 0.0))
  )

(defmacro less-than-hack (a b)
  `(if (is-nan-hack ,a)
       #f
       (< ,a ,b)
       )
  )

