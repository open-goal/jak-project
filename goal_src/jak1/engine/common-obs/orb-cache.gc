    (new 'stack-no-clear 'vector)
    (set! (-> self basetrans y) (+ 2048.0 (-> self root-pos)))
    (anim-loop)
    (none)
    )
  :post (the-as (function none :behavior orb-cache-top) plat-post)
  )

