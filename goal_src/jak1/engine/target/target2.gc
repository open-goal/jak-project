                            )
                        (set! (-> s3-0 x) (- (-> sv-40 x) (-> v1-30 x)))
                        (set! (-> s3-0 z) (- (-> sv-40 z) (-> v1-30 z)))
                        (let ((f30-0 (vector-xz-length s3-0)))
                          (if (< 122880.0 f30-0)
                              (vector-xz-normalize! s3-0 (the-as float 122880.0))
