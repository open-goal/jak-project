      (kill-current-level-hint '() '() 'exit)
      (process-spawn level-hint s3-1 arg1 arg2 :to arg3)
      )
    )
  0
  (none)
  )

(defun ambient-hint-spawn ((arg0 string) (arg1 vector) (arg2 process-tree) (arg3 symbol))
