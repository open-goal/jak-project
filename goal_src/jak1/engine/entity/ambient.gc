                                                              )
                                                             ((= v1-2 7)
                                                              "close-need-hint"
                                                              )
                                                             ((= v1-2 6)
                                                              "if-need-reward-speech"
                                                              )
                                                             ((= v1-2 5)
                                                              "if-need-reminder-a"
                                                              )
                                                             ((= v1-2 4)
                                                              "if-need-reminder"
                                                              )
                                                             ((= v1-2 3)
