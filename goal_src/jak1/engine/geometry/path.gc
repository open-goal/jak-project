            (set! f28-0
                  (+ (/ (vector-vector-xz-distance s2-0 s5-0) (vector-vector-xz-distance s4-0 s5-0)) (the float s1-1))
                  )
            )
          )
        )
      )
    f28-0
    )
  )

(defmethod path-control-method-20 path-control ((this path-control))
