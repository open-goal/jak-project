    )
  )

(defun vector-lerp-clamp! ((out vector) (a vector) (b vector) (alpha float))
  "Linearly interpolate between two vectors, clamping alpha to 0, 1
   w will be set to 1."
  (rlet ((vf0 :class vf)
         (vf1 :class vf)
         (vf2 :class vf)
         (vf3 :class vf)
         (vf4 :class vf)
         )
    (init-vf0-vector)
    (cond
      ((>= 0.0 alpha)
       (set! (-> out quad) (-> a quad))
       )
      ((>= alpha 1.0)
       (set! (-> out quad) (-> b quad))
       )
      (else
       (let ((v1-2 out))
         (let ((f0-2 alpha))
           (.lvf vf1 (&-> a quad))
           (.lvf vf2 (&-> b quad))
           (let ((a1-1 f0-2))
             (.mov vf4 a1-1)
             )
           )
         (.add.x.vf vf3 vf0 vf0 :mask #b1000)
         (.sub.vf vf2 vf2 vf1)
         (.mul.x.vf vf2 vf2 vf4)
         (.add.vf vf3 vf1 vf2 :mask #b111)
         (.svf (&-> v1-2 quad) vf3)
         )
       )
      )
    out
    )
  )

