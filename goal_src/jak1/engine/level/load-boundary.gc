    (cond
      ((not s5-0) (format *stdcon* "No load boundary selected - use Player 2 pad to select~%"))
      (else
       (format *stdcon* "Selected load boundary ~X~%" s5-0)
       (if (logtest? (-> s5-0 flags) (load-boundary-flags player))
