              (+! (-> s5-0 data (-> gp-0 vertex) x) (* f0-10 (-> s4-0 x)))
              (+! (-> s5-0 data (-> gp-0 vertex) z) (* f0-10 (-> s4-0 z)))))
           (set! (-> s5-0 tri-cnt) 0)
           0)
          ((cpad-hold? 1 r1) (if s5-0 (+! (-> s5-0 top-plane) f0-10)))
