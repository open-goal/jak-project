       (nav-enemy-set-hit-from-direction arg0)
       (cond
         ((-> self buddy-on-shoulders?)
          (let ((s4-0 (new 'stack-no-clear 'vector)))
            (vector-! s4-0 (target-pos 0) (-> self collide-info trans))
