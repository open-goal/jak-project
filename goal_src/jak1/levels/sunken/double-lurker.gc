              ((and (>= (-> s4-0 y) 8192.0)
                    (>= 14563.556 (fabs (deg- (quaternion-y-angle (-> self collide-info quat)) (atan (-> s4-0 x) (-> s4-0 z)))))
                    )
               (when (send-event
                       arg0
