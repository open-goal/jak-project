    (ja-post)
    (go sun-iris-door-open)
    )
  :post (behavior ()
    (cond
      ((-> self move-to?)
       (set! (-> self move-to?) #f)
       (move-to-point! (-> self root-override) (-> self move-to-pos))
       (quaternion-copy! (-> self root-override quat) (-> self move-to-quat))
