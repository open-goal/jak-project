    )
  (set! (-> this path-speed-seek-speed)
        (* 2.0 (- (-> this path-normal-speed-hi) (-> this path-normal-speed-lo)))
        )
  (set! (-> this path-speed)
        (rand-vu-float-range (-> this path-normal-speed-lo) (-> this path-normal-speed-hi))
        )
  (set! (-> this targ-path-speed) (-> this path-speed))
  (let ((s4-0 (new 'stack-no-clear 'vector)))
