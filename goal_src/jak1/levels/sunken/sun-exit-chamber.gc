(defmethod init-from-entity! blue-eco-charger ((obj blue-eco-charger) (arg0 entity-actor))
  (set! (-> obj open-level) 0.0)
  (let ((s4-0 (new 'process 'collide-shape obj (collide-list-enum hit-by-player))))
