   (targ-dest  float       :offset-assert 364)
   (dest       float       :offset-assert 368)
   (speed      float       :offset-assert 372)
   (touch-time time-frame  :offset-assert 376)
   )
  :heap-base #x110
  :method-count-assert 33
  :size-assert         #x180
  :flag-assert         #x2101100180
  )


