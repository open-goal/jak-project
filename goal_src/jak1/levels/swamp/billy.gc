(defmethod relocate billy ((obj billy) (arg0 int))
  (countdown (v1-0 3)
    (if (nonzero? (-> obj path-data v1-0))
        (&+! (-> obj path-data v1-0) arg0)
        )
    )
  (the-as
    billy
    ((the-as (function process-drawable int process-drawable) (find-parent-method billy 7)) obj arg0)
    )
  )

