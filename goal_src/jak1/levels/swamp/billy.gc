    (none)
    )
  :trans (behavior ()
    (cond
      ((> (-> self num-snacks) 0)
       (when (process-grab? *target*)
         (close-current! (-> self tasks))
         (go-virtual play-anim)
         )
       )
      ((< (-> self num-snacks) 0)
       (go-virtual idle)
       )
      (else
        (process-grab? *target*)
        (when (and *target* (logtest? (-> *target* state-flags) (state-flags grabbed)))
          (let ((gp-0 (get-response (-> self query))))
            (cond
              ((and (= gp-0 'yes) (process-release? *target*))
