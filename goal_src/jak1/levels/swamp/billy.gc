                        (when v1-2
                          (set! (-> (the-as billy-rat v1-2) dest-type) (the-as uint 1))
                          (get-random-point (-> self path-waypts) (-> (the-as billy-rat v1-2) destination))
                          )
                        )
                      )
                     )
            )
    )
  :enter (behavior ()
    (init!
      (-> self query)
      (lookup-text! *common-text* (text-id play-again?) #f)
      40
      150
      30
      #f
      (lookup-text! *common-text* (text-id quit) #f)
