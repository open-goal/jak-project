               )
              )
            )
          )
        )
      )
    (spool-push *art-control* "billy-reject" 0 self -99.0)
    (none)
    )
  :code (the-as (function none :behavior billy) process-taskable-anim-loop)
  :post (behavior ()
    (ja-post)
    (none)
    )
  )

