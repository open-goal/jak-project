(defstate happy-plant-opening (happy-plant)
  :exit (behavior ()
    (when *target*
      (logclear! (-> *target* mask) (process-mask sleep))
      (process-entity-status! self (entity-perm-status bit-3) #f)
      (logior! (-> self mask) (process-mask actor-pause))
      )
