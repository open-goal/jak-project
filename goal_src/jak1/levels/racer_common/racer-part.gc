(defun part-hud-racer-speed-func ((arg0 sparticle-system) (arg1 sparticle-cpuinfo) (arg2 matrix))
  (let* ((f0-0 (-> *target* control unknown-float01))
         (f0-1 (cond
                 ((>= 122880.0 f0-0) (lerp-scale 42780.445 27488.71 f0-0 0.0 122880.0))
                 ((>= 131072.0 f0-0) (lerp-scale 27488.71 14927.645 f0-0 122880.0 131072.0))
                 ((>= 139264.0 f0-0) (lerp-scale 14927.645 7281.778 f0-0 131072.0 139264.0))
                 ((>= 147456.0 f0-0) (lerp-scale 7281.778 -546.13336 f0-0 139264.0 147456.0))
                 ((>= 155648.0 f0-0) (lerp-scale -546.13336 -6371.5557 f0-0 147456.0 155648.0))
                 ((>= 163840.0 f0-0) (lerp-scale -6371.5557 -10922.667 f0-0 155648.0 163840.0))
                 (else (lerp-scale -10922.667 -15473.777 f0-0 163840.0 172032.0)))))
    (cond
      ((< (-> *hud-parts* bike-speed 0 offset) 10)
       (if (< f0-1 (-> arg2 vector 1 z))
         (set! (-> arg2 vector 1 z) (deg-seek (-> arg2 vector 1 z) f0-1 (* 131072.0 (seconds-per-frame))))
         (set! (-> arg2 vector 1 z) (deg-seek (-> arg2 vector 1 z) f0-1 (* 32768.0 (seconds-per-frame))))))
      (else (set! (-> arg2 vector 1 z) f0-1))))
  (none))

