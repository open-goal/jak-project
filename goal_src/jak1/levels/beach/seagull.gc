             (f26-1 10.0)
             (v1-26 (/ (the-as int (rand-uint31-gen *random-generator*)) 256))
             (v1-27 (the-as number (logior #x3f800000 v1-26)))
             )
        (set! (-> s4-0 z) (+ f30-1 (* f28-1 (* f26-1 (+ -1.0 (the-as float v1-27))))))
        )
      (set! (-> s4-0 y) (-> obj trans y))
