  (set! (-> obj targetnum) (+ (-> arg0 extra perm user-int8 0) 1))
  (eval-path-curve-div! (-> obj path) (-> obj trans) (the float (+ (-> obj targetnum) -1)) 'interp)
  (set! (-> obj target quad) (-> obj trans quad))
