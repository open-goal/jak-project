         (suspend)
         (ja :num! (seek!))
         )
       )
      (else
        (ja-channel-push! 1 (seconds 0.1))
        )
      )
    (ja :group! muse-run-ja :num! min)
    (loop
      (suspend)
      (ja :num! (loop! (* 0.000016276043 (-> self momentum-speed))))
      )
    (none)
    )
  :post (behavior ()
    (set! (-> self nav destination-pos quad) (-> self dest-point quad))
    (dummy-19
      (-> self nav)
      (-> self nav target-pos)
      (-> self collide-info)
