(defstate quicksandlurker-wait (quicksandlurker)
  :event quicksandlurker-default-event-handler
  :trans (behavior ()
    (cond
      ((and *target* (>= 81920.0 (vector-vector-distance (-> self root trans) (-> *target* control trans))))
       (set! (-> self y-offset) 1228.8)
