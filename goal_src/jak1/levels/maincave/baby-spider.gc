                                         (arg3 symbol)
                                         (arg4 int)
                                         (arg5 int)
                                         (arg6 symbol)
                                         )
  (set! (-> obj hatched?) arg0)
  (set! (-> obj fast-start?) arg1)
  (set! (-> obj die-if-not-visible?) arg2)
  (set! (-> obj hack-move-above-ground?) arg3)
