      )
    (#when PC_PORT
      (when (and (= (get-response (-> self query)) 'no) (or (not (= *cheat-mode* 'debug)) (not (cpad-hold? 0 r1))) (= (-> self which-movie) 1))
          (format #t "skipped green-sagecage~%")
          (start 'play (get-continue-by-name *game-info* "citadel-elevator"))
