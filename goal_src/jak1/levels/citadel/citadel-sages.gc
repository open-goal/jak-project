          (return #f)
          ))
    (cond
      ((= (-> self which-movie) 1)
       (set-blackout-frames (seconds 100))
       )
      ((= (-> self which-movie) 2)
       (start 'play (get-continue-by-name *game-info* "citadel-elevator"))
       )
      )
    (set! (-> self draw bounds w) 10240.0)
