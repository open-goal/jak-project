(defun vector<-cspace! ((arg0 vector) (arg1 cspace))
  (rlet ((Q :class vf)
         (vf0 :class vf)
         (vf2 :class vf)
         )
   (init-vf0-vector)
   (.lvf vf2 (&-> (-> arg1 bone) transform vector 3 quad))
   (.div.vf Q vf0 vf2 :fsf #b11 :ftf #b11)
   (.wait.vf)
   (.mul.vf vf2 vf2 Q :mask #b111)
   (.nop.vf)
   (.nop.vf)
   (.mov.vf vf2 vf0 :mask #b1000)
   (.svf (&-> arg0 quad) vf2)
   arg0
   )
  )