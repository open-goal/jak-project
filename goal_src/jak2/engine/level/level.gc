                )
            (set! lev-for-vis a2-32)
            (+! num-vis-levs 1)
            )
          )
        )
      )
    (if (and (>= num-vis-levs 1) (!= (-> lev-for-vis name) (-> obj vis-nick)))
        (want-vis-level obj (-> lev-for-vis name))
        )
    )
