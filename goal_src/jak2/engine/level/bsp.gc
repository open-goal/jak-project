      ;   (the-as uint (-> v1-0 vis-bits))
      ;   (the-as uint a2-0)
      ;   #f
      ;   )
      )
    (let ((at-0 *math-camera*))
      (with-vf (vf16 vf17 vf18 vf19 vf20 vf21 vf22 vf23 vf24 vf25 vf26 vf27 vf28 vf29 vf30 vf31)
               :rw 'write
               (.lvf vf16 (&-> at-0 plane 0 quad))
