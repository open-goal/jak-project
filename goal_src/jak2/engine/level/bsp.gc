         (vf27 :class vf)
         (vf28 :class vf)
         (vf29 :class vf)
         (vf30 :class vf)
         (vf31 :class vf)
         )
    (let ((v1-0 (-> obj level))
          (a2-0 (/ (+ (-> obj visible-list-length) 15) 16))
          )
      (__mem-move (-> (scratchpad-object terrain-context) work background vis-list) (-> v1-0 vis-bits) (the uint (* a2-0 16)))
      ; (dma-send-to-spr-no-flush
