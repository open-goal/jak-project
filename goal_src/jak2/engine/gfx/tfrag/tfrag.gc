      (+! (-> arg0 data s4-0 used) v1-22)
      (+! (-> arg0 data s4-0 total) (logand -16 (+ v1-22 15)))
      )
    (set! (-> arg0 data (+ s4-0 1) name) "tfragment-base")
    (+! (-> arg0 data (+ s4-0 1) count) 1)
    (let ((v1-33 (* (-> obj dma-qwc 0) 16)))
