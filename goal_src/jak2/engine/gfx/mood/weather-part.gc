    )
  (set! (-> *part-id-table* 56 init-specs 19 initial-valuef) (+ 32768.0 (vector-y-angle arg2)))
  (let ((t9-2 sp-launch-particles-var)
        (a0-3 *sp-particle-system-2d*)
        (a1-2 (-> *part-id-table* 58))
        (a2-2 *launch-matrix*)
