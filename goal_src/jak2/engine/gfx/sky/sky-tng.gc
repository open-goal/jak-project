    (set-sky-vf27 (&-> obj giftag-haze quad))

    ;; convert to sky format
    (let ((s4-0 (the-as object (-> arg0 base))))
      (&+! (-> arg0 base) 16)
      (set! sv-16 *haze-vert-array*)
      (set! sv-20 *haze-poly*)
