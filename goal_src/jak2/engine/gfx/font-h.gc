;; TODO: figure out what's weird with set-font-color
;; The font system draws all of the strings.

(defenum font-color
  :type uint32
  (default 0)
  (one 1)
  (orange-red 3)
  )

;; copied from jak1, not completely confirmed.
(defenum font-flags
  :type uint32
  :bitfield #t
  (shadow 0)
  (kerning 1)
  (middle 2)
  (left 3)
  (right 4)
  (large 5)
  (pc-hack 6)
  )

(define-extern draw-string-xy (function string dma-buffer int int font-color font-flags float))

