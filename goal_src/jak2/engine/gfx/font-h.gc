(defun font-set-tex0 ((arg0 (pointer gs-tex0)) (arg1 texture) (arg2 uint) (arg3 uint) (arg4 uint))
  "Write the TEX0 parameters for a font"
  (set! (-> arg0 0) (new 'static 'gs-tex0
                      :tcc #x1
                      :cld #x1
                      :cbp arg4
                      :th (+ (log2 (+ (-> arg1 h) -1)) 1)
                      :tw (log2 (-> arg1 w))
                      :tbw (-> arg1 width 0)
                      :tbp0 (/ (the-as int arg2) 64)
                      :psm arg3
                      )
        )
  (none)
  )

