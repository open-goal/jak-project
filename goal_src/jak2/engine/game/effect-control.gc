              )
          (return 0)
          )
      (when *debug-effect-control*
        (set! sv-128 s0-0)
        (string<-charp (clear *temp-string*) (the-as (pointer uint8) (& sv-128)))
        (format
          #t
          "(~5D) effect sound ~A ~A (~S) frame ~F joint ~D "
          (-> pp clock frame-counter)
          (-> obj process name)
