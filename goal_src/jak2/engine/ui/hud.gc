       (set! (-> self flags) (the-as hud-flags v0-1))
       v0-1
       )
      (else
        (event-callback self proc arg1 event-type event)
        )
      )
    )
  :enter (behavior ()
    (set-action!
      *gui-control*
      (gui-action hidden)
      (-> self gui-id)
      (gui-channel none)
      (gui-action none)
      (the-as string #f)
      (the-as (function gui-connection symbol) #f)
      (the-as process #f)
      )
    (set! (-> self offset) 1.0)
    (let ((gp-0 (-> self child)))
      (while gp-0
        (send-event (ppointer->process gp-0) 'draw #f)
        (set! gp-0 (-> gp-0 0 brother))
        )
      )
    (none)
    )
  :code (the-as (function none :behavior hud) sleep-code)
  :post (behavior ()
    (if (logtest? (-> self flags) (hud-flags should-die))
        (deactivate self)
