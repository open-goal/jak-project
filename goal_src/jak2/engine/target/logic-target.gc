(in-package goal)

;; name: logic-target.gc
;; name in dgo: logic-target
;; dgos: ENGINE, GAME

;; DECOMP BEGINS

(defun stop ((arg0 symbol))
  "Kill the target process, set the game mode to the specified mode."
  (when *target*
    (kill-by-name "target" *active-pool*)
    (set! *target* #f)
    )
  (set! (-> *game-info* mode) arg0)
  0
  )