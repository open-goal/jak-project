         )
       )
      (('rider-on)
       (let ((s5-3 (-> self child)))
         (while s5-3
           (send-event (ppointer->process s5-3) 'rider-on)
           (set! s5-3 (-> s5-3 0 brother))
           )
         )
       )
      (else
        (set! gp-0 #f)
        )
      )
    gp-0
    )
  )

