      )
    :load-point-obj "strip-start"
    :end-point-obj (new 'static 'continue-point
      :name "strip-start"
      :level #f
      :trans (new 'static 'vector :x 10374168.0 :y 303140.03 :z -300916.75 :w 1.0)
      :quat (new 'static 'vector :y 0.0218 :w -0.9997)
