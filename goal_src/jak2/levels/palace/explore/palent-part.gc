(defun palent-deactivate ()
  "Sets [[palshaft.bigmap-id]] to `20`"
  (set! (-> palshaft bigmap-id) (the-as uint 20))
  0
  (none)
  )

(defun palent-activate ()
  "Sets [[palshaft.bigmap-id]] to `12`"
  (set! (-> palshaft bigmap-id) (the-as uint 12))
  0
  (none)
  )
