    (send-event (handle->process (-> self race-state hud-turbo-counter)) 'force-hide)
    (process-spawn hud-race-final-stats :init hud-init-by-other :to self)
    (set-time! (-> self state-time))
    (until #f
      (draw-message-retry self)
