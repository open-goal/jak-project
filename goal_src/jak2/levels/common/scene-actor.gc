  ()
  )


(defmethod init-art! ((this youngsamos-npc))
  "@see [[initialize-skeleton]]"
  (initialize-skeleton
    this
    (the-as skeleton-group (art-group-get-by-name *level* "skel-youngsamos-highres" (the-as (pointer uint32) #f)))
    (the-as pair 0)
    )
  (if (zero? (-> this draw light-index))
      (set! (-> this draw light-index) (the-as uint 30))
      )
  0
  (none)
  )

(defmethod process-taskable-method-32 ((this youngsamos-npc))
  (case (-> this task actor)
    (((game-task-actor youngsamos-forest))
     (when (not (task-node-closed? (game-task-node forest-protect-resolution)))
       (let ((a0-3 (-> this skel root-channel 0)))
         (set! (-> a0-3 frame-group) (the-as art-joint-anim (-> this draw art-group data 4)))
         (set! (-> a0-3 frame-num) 0.0)
         (joint-control-channel-group! a0-3 (the-as art-joint-anim (-> this draw art-group data 4)) num-func-identity)
         )
       (ja-post)
       (let ((s5-0 (new 'stack-no-clear 'task-arrow-params)))
         (set! (-> s5-0 pos quad) (-> (vector<-cspace! (new 'stack-no-clear 'vector) (-> this node-list data 3)) quad))
         (quaternion-copy!
           (-> s5-0 quat)
           (eul->quat (new 'stack-no-clear 'quaternion) (new 'static 'euler-angles :x -16384.0 :y 16384.0))
           )
         (set! (-> s5-0 flags) (task-arrow-flags))
         (set! (-> s5-0 map-icon) (the-as uint 15))
         (task-arrow-spawn s5-0 this)
         )
       )
     )
    )
  (call-parent-method this)
