    ((lambda :behavior spydroid
       ()
       (let ((gp-0 (new 'stack 'joint-exploder-tuning (the-as uint 0))))
         (process-spawn
           joint-exploder
           (art-group-get-by-name *level* "skel-spydroid-exploding" (the-as (pointer uint32) #f))
           20
           gp-0
           *spydroid-exploder-params*
