        )
    (metalkor-bomb-ground-check)
    (metalkor-bomb-collide)
    (none)
    )
  :code (the-as (function none :behavior metalkor-bomb) sleep-code)
  :post (the-as (function none :behavior metalkor-bomb) ja-post)
  )
