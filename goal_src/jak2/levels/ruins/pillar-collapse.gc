                            ((= a0-8 'crush)
                             (< (sqrtf (+ (* (-> v1-4 x) (-> v1-4 x)) (* (-> v1-4 z) (-> v1-4 z)))) 40960.0)
                             )
                            ((= a0-8 'debug)
