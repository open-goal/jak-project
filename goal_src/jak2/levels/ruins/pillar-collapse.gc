                             )
                            )
                          )
                    )
               (cond
                 (v1-6
                   (go-virtual hit)
                   )
                 (else
                   (go-virtual bump)
                   #f
                   )
                 )
               )
             )
           )
         )
       )
      (('look-at-point)
       (send-event (ppointer->process (-> self child)) 'look-at-point)
       )
      (('combo)
       #t
       )
      )
    )
  :enter (behavior ()
    (let ((s5-0 (matrix<-transformq! (new 'stack-no-clear 'matrix) (the-as transformq (-> self root-override trans)))))
      (if (string= (-> self name) "ruins-pillar-collapse-9")
