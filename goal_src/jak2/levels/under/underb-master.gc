  (set! (-> self air-charge-up?) #f)
  (set! (-> self ambient-sound-id) (new 'static 'sound-id))
  (set! (-> self big-room-entered) #f)
  (set! (-> self under-plat-player-on) #f)
  (set! (-> self under-plat-is-up) #f)
  (go-virtual idle)
  (none)
  )

