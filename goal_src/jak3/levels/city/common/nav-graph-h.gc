(defmethod node-at-idx ((this nav-graph) (arg0 int))
  (let ((v0-0 (the-as nav-node #f)))
    (if (and (>= arg0 0) (< arg0 (-> this node-count)))
        (set! v0-0 (-> this node-array arg0))
        )
    v0-0
    )
  )
