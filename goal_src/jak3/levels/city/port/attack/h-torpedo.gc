(defmethod find-ground-height ((this htorpedo))
  (let ((s5-0 (new 'stack-no-clear 'collide-query)))
    (let ((f1-0 225280.0)
          (f2-0 8192.0)
          (f0-0 1024.0)
          )
      (set! (-> s5-0 start-pos quad) (-> this rbody position quad))
      (+! (-> s5-0 start-pos y) f2-0)
      (vector-reset! (-> s5-0 move-dist))
