  :post (behavior ()
    (if (nonzero? (-> self sound))
        (update! (-> self sound))
        )
    (ja-post)
    )
  )

(defmethod init-from-entity! ((this waspala-paddle-wheel) (arg0 entity-actor))
  (set! (-> this root) (new 'process 'trsqv))
  (process-drawable-from-entity! this arg0)
