    crawl
    attack0
    attack1
    ambush-crawling
    ambush-jumping
    undefined
    roll-right
    roll-left
    hop-away
    )
  (:methods
    (mantis-method-199 (_type_) none)
    (mantis-method-200 (_type_) none)
    (mantis-method-201 (_type_ vector vector) symbol)
    (mantis-method-202 (_type_ process-focusable vector) none)
