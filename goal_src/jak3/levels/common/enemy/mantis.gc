(deftype mantis-jump-info (structure)
  ((distance     float)
   (search-step  uint32)
   (destination  vector  :inline)
   (direction    uint16)
   (start-anim   uint32)
   (air-anim     uint32)
   (land-anim    uint32)
   )
  )


(deftype mantis (nav-enemy)
  ((base-height   float)
   (flags         mantis-flag)
   (attack-timer  time-frame        :offset 632)
   (track-timer   time-frame)
   (gspot-timer   time-frame)
