                           :dma-vif (new 'static 'dma-packet
                             :dma (new 'static 'dma-tag :qwc #x4 :id (dma-tag-id cnt))
                             :vif1 (new 'static 'vif-tag :imm #x4 :cmd (vif-cmd direct) :msk #x1)
