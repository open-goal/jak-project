    (set! (-> self icons v1-22 scale-x) 1.0)
    (set! (-> self icons v1-22 scale-y) 1.0)
    )
  (dotimes (v1-25 8)
    (set! (-> self values v1-25 current) -1)
    (set! (-> self values v1-25 target) 0)
    )
  (init-callback self)
  (set! (-> self event-hook) hud-hidden-event-handler)
  (go hud-hidden)
  )

