(defmethod check-ready-and-maybe-show ((this hud) (arg0 symbol))
  (case (get-status *gui-control* (-> this gui-id))
    (((gui-status ready) (gui-status active))
     (if arg0
         (set-action!
           *gui-control*
           (gui-action play)
           (-> this gui-id)
           (gui-channel none)
           (gui-action none)
           (the-as string #f)
           (the-as (function gui-connection symbol) #f)
           (the-as process #f)
           )
         )
     #t
     )
    (else
      #f
      )
    )
  )

