      (set! (-> a1-3 sync-flags) (the-as sync-flags v1-5))
      )
    (set! (-> a1-3 entity) arg0)
    (set! (-> a1-3 period) (the-as uint #x6fc23b000))
    (set! (-> a1-3 percent) 0.0)
