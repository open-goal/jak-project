  (stack-size-set! (-> this main-thread) 32)
  (let ((a0-3 (the-as string ((method-of-type res-lump get-property-struct)
                              arg0
                              'sound-name
                              'interp
                              -1000000000.0
                              ""
                              (the-as (pointer res-tag) #f)
                              *res-static-buf*
