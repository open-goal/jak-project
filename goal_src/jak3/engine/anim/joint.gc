        )
      (the-as joint #f)
      )
    (else
      (dotimes (s4-1 (-> this length))
        (if (name= arg0 (-> this data s4-1 name))
            (return (the-as basic (-> this data s4-1)))
            )
        )
      (the-as joint #f)
