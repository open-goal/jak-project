      s5-0
      s4-0
      s3-0
      s1-0
      (if v1-1
          (-> v1-1 frames fixed)
          0
          )
      (if (-> this anim)
          (sar
            (used-bytes-for-slot
              *anim-manager*
              (the-as int (/ (the-as int (- (the-as uint this) (the-as uint (the-as uint (-> *anim-manager* slot))))) 16))
              )
