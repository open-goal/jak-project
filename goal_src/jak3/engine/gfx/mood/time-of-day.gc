  (rlet ((acc :class vf)
         (vf0 :class vf)
         (vf4 :class vf)
         (vf5 :class vf)
         (vf6 :class vf)
         )
    (init-vf0-vector)
    (.mov vf6 arg2)
    (.lvf vf4 (&-> arg0 quad))
    (.lvf vf5 (&-> arg1 quad))
    (.mul.w.vf acc vf4 vf0)
    (.add.mul.x.vf vf4 vf5 vf6 acc)
    (.svf (&-> arg0 quad) vf4)
    (.mov v0-0 vf4)
    v0-0
    )
  )

