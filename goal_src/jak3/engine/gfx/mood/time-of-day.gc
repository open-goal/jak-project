                )
              (cond
                ((or (and (or (>= 7.0 (-> self time-of-day)) (>= (-> self time-of-day) 17.0)) s5-1) s4-1)
                 (if (zero? (-> self moon-count))
                     (spawn (-> self moon) gp-0)
