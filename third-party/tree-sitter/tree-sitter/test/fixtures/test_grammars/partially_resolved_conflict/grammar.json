{
  "name": "partially_resolved_conflict",

  "rules": {
    "expression": {
      "type": "CHOICE",
      "members": [
        {"type": "SYMBOL", "name": "binary"},
        {"type": "SYMBOL", "name": "identifier"}
      ]
    },

    "unary_a": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {"type": "STRING", "value": "!"},
          {"type": "SYMBOL", "name": "expression"}
        ]
      }
    },

    "unary_b": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {"type": "STRING", "value": "!"},
          {"type": "SYMBOL", "name": "expression"}
        ]
      }
    },

    "binary": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {"type": "SYMBOL", "name": "unary_a"},
            {"type": "SYMBOL", "name": "unary_b"},
            {"type": "SYMBOL", "name": "expression"}
          ]
        },
        {"type": "STRING", "value": "<"},
        {"type": "SYMBOL", "name": "expression"}
      ]
    },

    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    }
  }
}
