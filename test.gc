(defun vector-cross! ((arg0 vector) (arg1 vector) (arg2 vector))
  (rlet ((vf1 :class vf)
         (vf2 :class vf)
         (vf3 :class vf))
    (.lvf vf1 arg1)
    (.lvf vf2 arg2)
    (.outer.product.vf vf3 vf1 vf2)
    (.svf arg0 vf3)
    arg0))

(let ((vf1 (new 'static 'vector :x 1.0 :y 2.0 :z 3.0 :w 4.0))
      (vf2 (new 'static 'vector :x 5.0 :y 6.0 :z 7.0 :w 8.0))
      (vf3 (new 'static 'vector :x 0.0 :y 0.0 :z 0.0 :w 999.0)))
  (vector-cross! vf3 vf1 vf2)
  (format #t "vector-cross! => (~f, ~f, ~f, ~f)~%" (-> vf3 x) (-> vf3 y) (-> vf3 z) (-> vf3 w)))